{
  "compilerOptions": {
    "target": "ES2021",
    "module": "NodeNext",
   "moduleResolution": "NodeNext",
    "strict": true,
    "types": ["@cloudflare/workers-types"],
    "allowJs": false,
    "isolatedModules": true,
 "ignoreDeprecations":"6.0",

  
    "baseUrl": "./src",
    "paths": {
      "@domains/*": ["domains/*"],
      "@routes/*": ["routes/*"],
      "@utils/*": ["utils/*"],
      "@lib/*": ["lib/*"],
      "@types/*": ["types/*"]
    }
  },
  "include": ["src"]
}
